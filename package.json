    "axios-retry": "^3.1.2",
    "bowser": "^2.7.0",
    "devtools-detect": "^3.0.0",
    "firebase": "^7.4.0",
    "jszip": "^3.2.2",
    "reconnectingwebsocket": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
